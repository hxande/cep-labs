  },
  "servers": [
    {
      "url": "http://localhost:3333/",
      "description": "Local server"
    }
  ],
  "tags": [
    {
      "name": "CEP"
    }
  ],
  "paths": {
    "/ceps/{id}": {
      "get": {
        "tags": [
          "CEP"
        ],
        "description": "Buscar endereço pelo CEP",
        "operationId": "searchCep",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "description": "Código do CEP a ser buscado"
          }
        ],
        "responses": {
          "200": {
            "description": "Endereço recuperado pelo CEP informado",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "cep": {
                      "type": "string"
                    },
                    "logradouro": {
                      "type": "string"
                    },
                    "complemento": {
                      "type": "string"
                    },
                    "bairro": {
                      "type": "string"
                    },
                    "localidade": {
                      "type": "string"
                    },
                    "uf": {
                      "type": "string"
                    },
                    "ibge": {
                      "type": "string"
                    },
                    "gia": {
                      "type": "string"
                    },
                    "ddd": {
                      "type": "string"
                    },
                    "siafi": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "CEP inválido"
          },
          "404": {
            "description": "Objeto não encontrado"
          }
        }
      }
    }
  }
}