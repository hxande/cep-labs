{
  "openapi": "3.0.0",
  "info": {
    "version": "1.3.0",
    "title": "CEP Labs",
    "description": "API para desafio",
    "termsOfService": "https://smartbear.com/terms-of-use/",
    "contact": {
      "name": "Hugo Alexandre",
      "email": "h.alexandre014@gmail.com",
